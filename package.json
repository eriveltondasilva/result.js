{
  "name": "result.js",
  "description": "A lightweight, Rust-inspired Result type for JavaScript and TypeScript â€” handle success and error cases without exceptions.",
  "version": "1.0.0",
  "keywords": [
    "result",
    "either",
    "typescript",
    "javascript",
    "functional programming",
    "monad",
    "error handling"
  ],
  "license": "MIT",
  "author": {
    "name": "Erivelton Silva",
    "email": "eriveltondasilva13@gmail.com"
  },
  "homepage": "https://github.com/eriveltondasilva/result.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eriveltondasilva/result.js.git"
  },
  "bugs": {
    "url": "https://github.com/eriveltondasilva/result.js/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsup",
    "build:release": "npm run clean && tsup --minify",
    "prepublishOnly": "npm run build:release",
    "dev": "tsx src/index.ts",
    "fmt": "biome format --write --verbose",
    "lint": "biome lint --write --verbose",
    "check": "biome check --write --verbose",
    "types": "tsc --noEmit",
    "ci": "biome ci",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@tsconfig/node22": "^22.0.2",
    "@tsconfig/strictest": "^2.0.7",
    "@types/node": "^24.10.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}
