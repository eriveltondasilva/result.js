{
  "$schema": "https://typedoc.org/schema.json",

  "name": "Result.js",
  "out": "./docs/typedoc",
  "entryPoints": ["./src/index.ts"],
  "projectDocuments": ["./docs/*.md"],
  "plugin": [
    "typedoc-plugin-missing-exports",
    "typedoc-github-theme"
  ],
  "includeVersion": true,

  "navigation": {
    "includeCategories": false,
    "includeGroups": false,
    "includeFolders": true,
    "compactFolders": false,
    "excludeReferences": true
  },
  "navigationLinks": {
    "github": "https://github.com/eriveltondasilva/result.js",
    "npm": "https://www.npmjs.com/package/result.js",
    "docs": "https://eriveltondasilva.github.io/result.js/"
  },
}